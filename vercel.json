{
  "routes": [
    {
      "src": "^/sw.js$",
      "dest": "/_next/public/sw.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "service-worker-allowed": "/"
      }
    },
    {
      "src": "^/(favicon.ico|robots.txt|sitemap.xml)$",
      "dest": "/$1"
    }
  ],
  "builds": [
    {
      "src": "next.config.mjs",
      "use": "@vercel/next"
    }
  ]
}
